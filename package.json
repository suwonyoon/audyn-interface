{
  "name": "audyn",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:office": "vite --mode office",
    "build": "npm run build:web && npm run build:office",
    "build:web": "tsc -b && vite build",
    "build:office": "tsc -b && vite build --mode office",
    "lint": "eslint .",
