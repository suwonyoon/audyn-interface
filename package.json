    "sideload": "office-addin-debugging start manifests/manifest.xml",
    "stop": "office-addin-debugging stop manifests/manifest.xml"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "fabric": "^6.5.4",
    "pptxgenjs": "^3.12.0",
    "jszip": "^3.10.1",
    "fast-xml-parser": "^4.5.1",
    "zustand": "^5.0.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "file-saver": "^2.0.5",
    "lucide-react": "^0.469.0"
  },
  "overrides": {
    "tar": "^7.5.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/file-saver": "^2.0.7",
    "@types/office-js": "^1.0.377",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
